  "scripts": {
    "dist": "NODE_ENV=production webpack -p --progress --display-optimization-bailout --config webpack.config.js",
    "pack": "webpack --progress --display-optimization-bailout --config webpack.config.js",
    "start": "webpack-dev-server --hot --history-api-fallback --content-base public/",
    "watch": "webpack --watch --progress --config webpack.config.js",
    "json2pot": "rip json2pot 'app/i18n/messages/**/*.json' -o app/i18n/locales/ui.pot",
    "po2json": "rip po2json app/i18n/locales/${LOCALE}.po -m 'app/i18n/messages/**/*.json' -o app/i18n/locales/${LOCALE}.json",
    "tx:push": "npm run json2pot && tx push -s",
    "tx:pull": "tx pull -a && for file in app/i18n/locales/*.po; do LOCALE=$(basename ${file%.po}) npm run po2json; done"
